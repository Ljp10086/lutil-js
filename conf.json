{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["./src/array", "./src/collections", "./src/function"],
    "exclude": ["./node_modules"],
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "template": "node_modules/better-docs"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  }
}